{
    "version": 2,
    "builds": [
      {
        "src": "*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/",
        "methods" : ["GET","POST"],
        "headres" : {
          "Access-Control-Allow-Origin" : "*"
        }
      }
    ]
  }
  